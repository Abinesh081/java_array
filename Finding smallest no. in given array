public class Array 
{
public static void main(String[] args) 
{
Array lb = new Array();
lb.find_smallest();
}
private void find_smallest() 
{
int[] age = {50, 100, 150, 25, 200}; 
int small = Integer.MAX_VALUE; // int small compare to array integer maximum value.
  for(int i=0; i<age.length; i++)
		{
		if(age[i]<small)	
		{
		small = age[i]; 
		}
		}
		System.out.println(small); 
}
}
